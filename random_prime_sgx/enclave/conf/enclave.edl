/* Enclave.edl - Top EDL file. */

enclave {

    untrusted {
        
    };


    trusted {
        /* 
         * ECALL: get_random_int
         * Returns a random integer generated securely inside the enclave.
         */
        public int get_random_int(void);

        /* 
         * ECALL: is_prime
         * Checks if the input integer 'n' is a prime number.
         * Returns 1 if prime, 0 otherwise.
         */
        public int is_prime(int n);
    };
};
